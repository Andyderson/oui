    "postcss-loader": "^0.13.0",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-docgen": "^2.8.2",
    "react-dom": "^15.1.0",
    "react-element-to-jsx-string": "^3.2.0",
    "require-dir": "^0.3.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.16.2",
    "yargs": "^4.1.0"
  },
  "scripts": {
    "test": "npm run eslint && npm run jest && codecov && npm run scss-lint && npm run icons:update && gulp sass && npm run react:build",
    "start": "bundle install && bundle update && npm install && npm run sass && npm run icons:update && npm run babel && npm run react:build",
    "jest": "jest --config .jest.config.json",
    "jest:watch": "jest --config .jest.config.json --watch",
    "eslint": "eslint --ext .jsx --ext .js . --ignore-path .gitignore --max-warnings 0",
    "scss-lint": "bundle exec scss-lint src/",
    "sass": "gulp sass && gulp extras:build",
    "babel": "babel src/ --out-dir dist/js --ignore \"tests/*.js, src/js, src/index.jsx\"",
    "icons:update": "npm update oui-icons && node ./scripts/sync-oui-icons/index.js",
    "html-examples": "gulp html-examples",
    "preversion": "git checkout master && git pull --rebase=false && npm install",
    "postversion": "npm run postversion:publish && npm run postversion:update-devel",
    "postversion:publish": "git push --follow-tags",
    "postversion:update-devel": "git checkout devel && git pull && git merge master && git push origin devel && git checkout master",
    "react:build": "npm-run-all react:build:json --parallel react:build:js react:build:html",
    "react:build:js": "webpack --progress",
    "react:build:json": "react-docgen src/components/ -i tests -i example -i Icon > docs/data.json",
    "react:build:html": "mkdir -p dist/docs/ && cp -v ./docs/index.html ./dist/docs/index.html",
    "react:watch": "npm-run-all --parallel react:build:json react:build:html --parallel react:watch:js",
    "react:watch:js": "webpack-dev-server --content-base dist/docs --open --progress"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/optimizely/oui.git"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "author": "Tom Genoni <tom@atomeye.com> (http://atomeye.com/)",
